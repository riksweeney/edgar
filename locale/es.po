msgstr "se requiere %s"

#: src/item/bomb_pile.h:20
msgid "Bomb"
msgstr "Bomba"

#: src/enemy/rock.c:74
msgid "Small Rock"
msgstr "Roca pequeña"

#: src/item/containment_unit_controls.c:123
msgid "Press Action to retrieve the Purple Gem"
msgstr "Pulsa Acción para recuperar la Gema Púrpura"

#: src/item/tesla_charger.c:146
msgid "Tesla Pack"
msgstr "Paquete Tesla"

#: src/item/tesla_charger.c:169
msgid "Press Action to retrieve Tesla Pack"
msgstr "Pulsa Acción para recuperar el Paquete Tesla"

#: src/item/tesla_charger.c:178
msgid "Press Action to replace Tesla Pack"
msgstr "Pulse Acción para sustituir el Paquete Tesla"

#: src/item/slime_potion_pool.c:94
msgid "Obtained %s"
msgstr "Obtenido %s"

#: data/scripts/tutorial_objective.dat:1
msgid "Complete"
msgstr "Completado"

#: src/item/arrow_target.c:177
msgid "%d more to go..."
msgstr "%d más para llegar"

#: src/item/light_pillar.c:109
msgid "Used %s"
msgstr "Usado %s"

#: src/player.c:1040
msgid "Cannot equip items whilst transmogrified"
msgstr "No se pueden equipar objetos mientras se está transmutado"

#: src/player.c:1079
msgid "Equipped %s"
msgstr "Equipado %s"

#: src/player.c:1710
msgid "Maximum health has increased!"
msgstr "¡La salud máxima ha aumentado!"

#: src/player.c:2447
msgid "Bow will now fire %s"
msgstr "El arco ahora disparará %s"

#: src/player.c:2466
msgid "A bow is required to use this item"
msgstr "Necesitas un arco para usar este objeto"

#: src/player.c:2513
msgid "Cannot transmogrify here..."
msgstr "No me puedo transmutar aquí..."

#: data/scripts/jacob.dat:46
msgid "Yes"
msgstr "Sí"

#: data/scripts/cave_boss_die.dat:11
msgid "No"
msgstr "No"

#: src/menu/main_menu.c:336
msgid "Start a new game?"
msgstr "¿Empezar una nueva partida?"

#: src/menu/main_menu.c:351
msgid "Play the tutorial?"
msgstr "¿Jugar al tutorial?"

#: src/menu/main_menu.c:366
msgid "Exit the game?"
msgstr "¿Abandonar el juego?"

#: data/scripts/olaf.dat:11
msgid "Back"
msgstr "Atrás"

#: src/menu/io_menu.c:260
