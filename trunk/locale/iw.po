msgstr "סאונד"

msgid "SFX Volume"
msgstr "נפח SFX"

msgid "Music Volume"
msgstr "נפח מוסיקה"

msgid "Ice Spray"
msgstr "ספריי קרח"

msgid "Library"
msgstr "ספריה"

msgid "Right Wing Lift Key"
msgstr "Right Wing הרם קי"

msgid "Mine Entrance Door Key"
msgstr "שלי Entrance Door קי"

msgid "Village"
msgstr "הכפר"

msgid "Cave"
msgstr "מערה"

msgid "Fortress Outskirts"
msgstr "בפאתי המצודה"

msgid "Forbidden Swamp"
msgstr "אסור הביצה"

msgid "Sewage Restricted Zone Key"
msgstr "ביוב המוגבלים קי"

msgid "Basement"
msgstr "מרתף"

msgid "Fortress Ground Floor"
msgstr "מבצר קומת כניסה"

msgid "Sewer"
msgstr "ביוב"

msgid "Fortress Main Key"
