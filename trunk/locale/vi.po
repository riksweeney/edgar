msgid "Fortress Outskirts"
msgstr "Pháo đài ngoại ô"

msgid "Forbidden Swamp"
msgstr "Tử Cấm đầm lầy"

msgid "Sewage Restricted Zone Key"
msgstr "Giới hạn lý nước thải Khu chính"

msgid "Basement"
msgstr "Tầng hầm"

msgid "Fortress Ground Floor"
msgstr "Pháo đài Tầng trệt"

msgid "Sewer"
msgstr "Hệ thống thoát nước"

msgid "Fortress Main Key"
msgstr "Pháo đài chính khóa"

msgid "Teleporter Access Key 1"
